{
  "name": "Lost-Astronaut",
  "description": "Lost Astronaut site",
  "version": "1.0.0",
  "author": "Sebastian Stuber",
  "dependencies": {
    "bulma": "^0.6.2",
    "gatsby": "^1.9.169",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-less": "^1.0.9",
    "gatsby-plugin-manifest": "^1.0.13",
    "gatsby-plugin-netlify": "^1.0.15",
    "gatsby-plugin-offline": "^1.0.13",
    "gatsby-plugin-react-helmet": "^2.0.4",
    "gatsby-plugin-sharp": "^1.6.27",
    "gatsby-plugin-sitemap": "^1.2.11",
    "gatsby-remark-images": "^1.5.41",
    "gatsby-source-filesystem": "^1.5.15",
    "gatsby-transformer-remark": "^1.7.29",
    "gatsby-transformer-sharp": "^1.6.17",
    "react-helmet": "^5.2.0",
    "react-load-script": "0.0.6",
    "react-responsive-carousel": "^3.1.30",
    "typeface-poiret-one": "0.0.44",
    "typeface-roboto": "0.0.50"
  },
  "keywords": [
    "gatsby"
  ],
  "main": "n/a",
  "scripts": {
    "build": "npm run build:cms && npm run build:site",
    "build:site": "gatsby build",
    "build:cms": "cd cms && yarn && npm run build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "_format": "prettier --trailing-comma es5 --no-semi --single-quote --write",
    "format": "npm run format:src & npm run format:cms",
    "format:src": "npm run _format 'src/**/*.js'",
    "format:cms": "npm run _format 'cms/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.10.2"
  }
